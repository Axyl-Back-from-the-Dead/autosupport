  "devDependencies": {
    "@sapphire/eslint-config": "^5.0.4",
    "@sapphire/prettier-config": "^2.0.0",
    "@sapphire/ts-config": "^5.0.1",
    "@types/node": "^20.12.12",
    "@types/node-wit": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "bun-types": "^1.0.1",
